[deploy]

api1.server.com ansible_host=api1 ansible_python_interpreter=/usr/bin/python3 ansible_user=deploy
api2.server.com ansible_host=api2 ansible_python_interpreter=/usr/bin/python ansible_user=deploy

[deploy:vars]
